/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Not(in=a, out=nota);
    Not(in=b, out=notb);
    Or(a=a, b=b, out=outor);
    Or(a=nota, b=notb, out=notoutor);
    And(a=outor, b=notoutor, out=out);
}